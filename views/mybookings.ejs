<!doctype html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v6.1.1/css/all.css" integrity="sha384-/frq1SRXYH/bSyou/HUp/hib7RVN1TawQYja658FEOodR/FQBKVqT9Ol+Oz3Olq5" crossorigin="anonymous">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500&display=swap" rel="stylesheet">

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">
  <link rel="stylesheet" href="css/styles.css">
  <title>Dua Al-Madinah</title>

</head>

<body>
<%- include('navbar'); -%>




<div class="row">



  <div class="col-lg-8 table-responsive" style="padding:3% 7%;;">

      <h2>Here You can see all your previous bookings</h2>
      <br>



    <table class="table table-bordered table-hover">
           <thead>
             <tr>
               <th scope="col">S.NO</th>
               <th scope="col">Booking Date</th>
               <th scope="col">City Selected</th>
               <th scope="col">Package Selected</th>

               <th scope="col">See Deatails</th>
             </tr>
           </thead>
           <tbody>
             <%usersList.forEach(function(singleUser,index){%>
             <tr>
               <th scope="row"><%=index+1%></th>
               <td>
                    <label for="delete-City" class="form-label"><%=singleUser.booking_date%></label>

               </td>
               <td>
                    <label for="delete-City" class="form-label"><%=singleUser.city%></label>

               </td>
               <td>
                    <label for="delete-City" class="form-label"><%=singleUser.package%></label>

               </td>

               <td>
                 <form class="" action="seeStatus" method="post" autocomplete="off">
                 <button type="submit" name="giventoken" class="btn btn-outline-dark " value="<%=singleUser.tokenNumber%>">Status</button>
                  </form>

               </td>

             </tr>
             <%})%>
           </tbody>
         </table>

  </div>
  <div class="col-lg-4">
    <img src="images/tokenn.jpg" alt="" width="100%" style="padding:10%">

  </div>

</div>




  <%- include('footer');  -%>


  <script src="cityname.js">
    < script src = "https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js" >
  </script>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>


  </script>
  <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
</body>

</html>
